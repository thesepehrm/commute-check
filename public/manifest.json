{
  "manifest_version": 3,
  "name": "Commute Check",
  "version": "1.0.0",
  "description": "Check your commute time with ease.",
  "action": {
    "default_title": "Open sidebar",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    }
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["sidePanel", "runtime"],
  "host_permissions": ["https://maps.googleapis.com/"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
